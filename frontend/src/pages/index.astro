---
import BaseLayout from "../layouts/BaseLayout.astro";
import App from "../components/App";
export const prerender = false;
const params = Astro.url.searchParams;
---

<BaseLayout title={"View"}>
    <App
        searchParam={params.has("search") ? params.get("search") : ""}
        selectedParam={params.has("selected") ? params.get("selected") : ""}
        client:load
    />
</BaseLayout>
